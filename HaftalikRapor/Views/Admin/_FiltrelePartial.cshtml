@model HaftalikRapor.Models.RaporDB

<form action="/Admin/RaporFiltrele" method="post" enctype="multipart/form-data">
        @Html.AntiForgeryToken()
        <table style="float:right;">
            <tr>

                <td>
                     @Html.EditorFor(model => model.RaporBitisTarih, new { htmlAttributes = new { @class = "form-control", @type = "date", @style = "width:195px;" } })
                </td>
                <td>
                    <input type="submit" value="Bitiş Tarihine göre Filtrele" class="btn btn-danger" style="width:200px; height:30px;">
                </td>
            </tr>
        </table>
    </form>
    <br />
    <br />
    <form action="/Admin/RaporİsimFiltrele" method="post" enctype="multipart/form-data">
        @Html.AntiForgeryToken()
        <table style="float:right;">
            <tr>

                <td>
                    @Html.EditorFor(model => model.PersonelAd, new { htmlAttributes = new { @class = "form-control" } })
                </td>
                <td>
                    <input type="submit" value="İsime Göre Filtrele" class="btn btn-danger" style="width:200px; height:30px;" />
                </td>
            </tr>
        </table>
    </form>
